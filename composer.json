{
    "name": "juzaweb/videos",
    "type": "juzaweb-plugin",
    "license": "MIT",
    "homepage": "https://juzaweb.com",
    "description": "Manage your videos content with Juzaweb plugin",
    "extra": {
        "juzaweb": {
            "name": "Videos Management",
            "domain": "jw_videos",
            "providers": [
                "Juzaweb\\Videos\\Providers\\VideosServiceProvider"
            ],
            "networkable": true
        }
    },
    "autoload": {
        "psr-4": {
            "Juzaweb\\Videos\\": "src/"
        }
    },
    "require": {
        "aminyazdanpanah/php-ffmpeg-video-streaming": "^1.2",
        "google/apiclient": "^2.15"
    },
    "require-dev": {
        "juzaweb/modules": "^1.0",
        "juzaweb/dev-tool": "^1.0"
    }
}
